<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:vmu="http://schema.shipwreck.jp/viewmodel-utils/"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:v="clr-namespace:Shipwreck.ViewModelUtils.Controls">
    <ResourceDictionary.MergedDictionaries>
        <vmu:SharedResourceDictionary
            Source="pack://application:,,,/Shipwreck.ViewModelUtils.PresentationFramework;component/Themes/Generic.EntitySelectorComboBox.xaml" />
        <vmu:SharedResourceDictionary
            Source="pack://application:,,,/Shipwreck.ViewModelUtils.PresentationFramework;component/Themes/Generic.EntitySelectorView.xaml" />
        <vmu:SharedResourceDictionary
            Source="pack://application:,,,/Shipwreck.ViewModelUtils.PresentationFramework;component/Styles/Controls.CommandsButtons.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <Style
        TargetType="{x:Type v:CommandsButton}"
        BasedOn="{StaticResource CommandViewModel.CommandsButton.Medium}" />
</ResourceDictionary>