@namespace Shipwreck.ViewModelUtils.Components
@inherits ListComponentBase<MenuItemViewModel>
@using (OnBeginRender())
{
    if (Source?.Count > 0)
    {
        foreach (MenuItemViewModel e in Source)
        {
            <NavItem @key="@e.GetHashCode()"
                     DataContext="@e"
                     Source="@e.Children" />
        }
    }
}
